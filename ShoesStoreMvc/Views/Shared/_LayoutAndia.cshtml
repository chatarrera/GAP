<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <!-- CSS -->
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400italic,400">
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Droid+Sans">
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Lobster">
    <link rel="stylesheet" href="@Url.Content("~/Content/themes/andia/assets/")bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/themes/andia/assets/")font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/themes/andia/assets/")css/animate.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/themes/andia/assets/")css/magnific-popup.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/themes/andia/assets/")flexslider/flexslider.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/themes/andia/assets/")css/form-elements.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/themes/andia/assets/")css/style.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/themes/andia/assets/")css/media-queries.css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Javascript -->
    <script src="@Url.Content("~/Content/themes/andia/assets/")js/jquery-1.11.1.min.js"></script>
    <script src="@Url.Content("~/Content/themes/andia/assets/")bootstrap/js/bootstrap.min.js"></script>
    <script src="@Url.Content("~/Content/themes/andia/assets/")js/bootstrap-hover-dropdown.min.js"></script>
    <script src="@Url.Content("~/Content/themes/andia/assets/")js/jquery.backstretch.min.js"></script>
    <script src="@Url.Content("~/Content/themes/andia/assets/")js/wow.min.js"></script>
    <script src="@Url.Content("~/Content/themes/andia/assets/")js/retina-1.1.0.min.js"></script>
    <script src="@Url.Content("~/Content/themes/andia/assets/")js/jquery.magnific-popup.min.js"></script>
    <script src="@Url.Content("~/Content/themes/andia/assets/")flexslider/jquery.flexslider-min.js"></script>
    <script src="@Url.Content("~/Content/themes/andia/assets/")js/jflickrfeed.min.js"></script>
    <script src="@Url.Content("~/Content/themes/andia/assets/")js/masonry.pkgd.min.js"></script>
    <script src="http://maps.google.com/maps/api/js?sensor=true"></script>
    <script src="@Url.Content("~/Content/themes/andia/assets/")js/jquery.ui.map.min.js"></script>
    <script src="@Url.Content("~/Content/themes/andia/assets/")js/scripts.js"></script>
</head>

<body>
    <!-- Top menu -->
    <nav class="navbar" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#top-navbar-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" @Html.ActionLink("Home", "Index", "Home")
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="top-navbar-1">
                <ul class="nav navbar-nav navbar-right">
                    <li>
                        @Html.ActionLink("Store Items", "Index", "Article")<br><i class="fa fa-camera"></i>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    <div>
        @RenderBody()
    </div>
</body>
</html>
